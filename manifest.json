{
  "manifest_version": 2,
  "name": "make cats not covid",
  "version": "1.0",
  "description": "drunkathon pilot project",
  "permissions": [
    "storage"
  ],
  "icons": {
    "128": "images/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_idle",
      "all_frames": true,
      "js": [
        "content.js"
      ]
    }
  ]
}